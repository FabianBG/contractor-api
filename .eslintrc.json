{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
    },
    "extends": "standard",
    "overrides": [
    ],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["off", "always"],
        "comma-dangle": 0,
        "space-before-function-paren": 0
    }
}
